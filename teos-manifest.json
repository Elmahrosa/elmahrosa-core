{
  "schema": "teos.manifest.v1",
  "name": "elmahrosa-core",
  "repo": "Elmahrosa/elmahrosa-core",
  "role": "Core",
  "status": "active",
  "version": "0.1.0",
  "authority": {
    "icbc": "https://github.com/Elmahrosa/International-Civic-Blockchain-Constitution",
    "tesl_canonical": "https://github.com/Elmahrosa/International-Civic-Blockchain-Constitution/blob/main/LICENSE"
  },
  "stewardship": {
    "teos_forge": "https://github.com/Elmahrosa/TEOS-FORGE",
    "kernel": "https://github.com/Elmahrosa/elmahrosa-core"
  },
  "interfaces": {
    "provides": ["ecosystem.registry", "manifest.schema", "governance.policy"],
    "consumes": ["icbc.constitution", "tesl.license", "forge.stewardship"]
  },
  "compliance": {
    "jurisdictions": ["EG"],
    "data_sensitivity": "public",
    "controls": ["ICBC", "TESL_CANONICAL", "FORGE_STEWARDSHIP", "BRANCH_PROTECTION"]
  },
  "audit": {
    "events": ["registry.updated", "policy.updated", "schema.updated"],
    "evidence_export": { "formats": ["jsonl"], "retention_days": 365 }
  },
  "contacts": { "owner": "Ayman Seif", "security": "ayman@teosegypt.com" }
}
